<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff1de9b6"
    >

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="171dp"
        android:text="0"
        android:id="@+id/textView"
        android:background="#ffffff"
        android:textSize="70sp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/button7"
        android:text="7"
        android:layout_below="@+id/textView"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/button8"
        android:text="8"
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_centerVertical="true"
        android:layout_below="@+id/textView"
        android:layout_toRightOf="@+id/button7"
        android:layout_toEndOf="@+id/button7"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/button9"
        android:text="9"
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_centerVertical="true"
        android:layout_below="@+id/textView"
        android:layout_toRightOf="@+id/button8"
        android:layout_toEndOf="@+id/button8"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/button4"
        android:text="4"
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_centerVertical="true"
        android:layout_below="@+id/button7"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/button5"
        android:text="5"
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/button4"
        android:layout_below="@+id/button8"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/button6"
        android:text="6"
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/button5"
        android:layout_below="@+id/button9"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/button1"
        android:text="1"
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_centerVertical="true"
        android:layout_below="@+id/button4"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/button2"
        android:text="2"
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/button1"
        android:layout_below="@+id/button5"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/button3"
        android:text="3"
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/button2"
        android:layout_below="@+id/button6"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/buttond"
        android:text="."
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_centerVertical="true"
        android:layout_below="@+id/button1"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/button0"
        android:text="0"
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_toRightOf="@id/buttond"
        android:layout_centerVertical="true"
        android:layout_below="@id/button2"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="95dp"
        android:layout_height="85dp"
        android:id="@+id/buttonequl"
        android:text="="
        android:textColor="#E0DFE0"
        android:textSize="30sp"
        android:layout_toRightOf="@id/button0"
        android:layout_centerVertical="true"
        android:layout_below="@id/button3"
        android:background="#ff434343 "
        android:onClick="onClickNumber"
        android:maxWidth="85dp"
        />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:text="DEL"
        android:id="@+id/buttondel"
        android:textColor="#ffffff"
        android:textSize="25sp"
        android:background="#ff636363"
        android:layout_below="@+id/textView"
        android:layout_toRightOf="@+id/button9"
        android:layout_toEndOf="@+id/button9"
        android:onClick="onClickOperator"
        android:maxWidth="90dp" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:text="/"
        android:id="@+id/buttonDivide"
        android:textColor="#ffffff"
        android:textSize="25sp"
        android:background="#ff636363"
        android:layout_below="@+id/buttondel"
        android:layout_toRightOf="@+id/button6"
        android:onClick="onClickOperator"
        />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:text="*"
        android:id="@+id/buttonMul"
        android:textColor="#ffffff"
        android:textSize="25sp"
        android:background="#ff636363"
        android:layout_below="@+id/buttonDivide"
        android:layout_toRightOf="@+id/button6"
        android:onClick="onClickOperator"
        />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:text="-"
        android:id="@+id/buttonMinus"
        android:textColor="#ffffff"
        android:textSize="25sp"
        android:background="#ff636363"
        android:layout_below="@+id/buttonMul"
        android:layout_toRightOf="@+id/button6"
        android:onClick="onClickOperator"
        />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:text="+"
        android:id="@+id/buttonPlus"
        android:textColor="#ffffff"
        android:textSize="25sp"
        android:background="#ff636363"
        android:layout_below="@+id/buttonMinus"
        android:layout_toRightOf="@+id/buttonequl"
        android:onClick="onClickOperator"
        />
</RelativeLayout>